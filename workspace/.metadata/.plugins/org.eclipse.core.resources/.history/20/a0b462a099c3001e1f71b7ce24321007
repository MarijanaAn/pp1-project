program test302

namespace Addition{

	{
	
	int sum(int z, int d)
	{
		return z + d;
	}
	

	int inkrement(int a, int inc) 
		int pom; 
	{
		if(inc == 1) pom = 1;
		else if(inc == 2) pom = a;

		return a+inc;
	}
	
	}
}



int i;
bool ibool;

int niz2;

int niz[];
int a, curr;

{
	bool verify() {
		print('v');
		return false;
	}	
	
	void main()
	{

		
		if(i == 3 && verify()) print(-4);
		else print(4);  
		print(eol);
		print(Addition::sum(Addition::sum(niz[0], niz[1]), niz[2])); 	
		print(eol); 
		print(Addition::inkrement(niz[2], 2));
		a = 3;
		for (i=0; i < 3; i++){
			niz[i] = niz[i] * 2 * a;
		}
		for (i=0; i < 3; i++) print(niz[i]);
		print(eol);
		niz2 = 0;
